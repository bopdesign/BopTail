{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "boptail/stats",
  "title": "Stats",
  "description": "A custom stats block.",
  "category": "boptail",
  "icon": "chart-bar",
  "keywords": [
    "stats",
    "statistics"
  ],
  "acf": {
    "mode": "auto",
    "renderTemplate": "template.php",
    "validate": false
  },
  "attributes": {
    "align": {
      "type": "string",
      "default": "full"
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "alignContent": true,
    "alignText": true,
    "anchor": true,
    "color": {
      "background": false,
      "link": false,
      "text": false
    },
    "fullHeight": false,
    "html": false,
    "jsx": false,
    "multiple": false,
    "reusable": false
  },
  "example": {
    "attributes": {
      "mode": "preview",
      "data": {
        "_is_preview": "true"
      }
    }
  }
}
