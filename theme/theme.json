{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "color": {
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "palette": [
        {
          "slug": "background",
          "color": "#fff",
          "name": "Background"
        },
        {
          "slug": "foreground",
          "color": "#03282c",
          "name": "Foreground"
        },
        {
          "slug": "white",
          "color": "#fff",
          "name": "White"
        },
        {
          "slug": "black",
          "color": "#000",
          "name": "Black"
        },
        {
          "slug": "primary",
          "color": "#11615c",
          "name": "Primary"
        },
        {
          "slug": "secondary",
          "color": "#14e5c5",
          "name": "Secondary"
        }
      ]
    },
    "layout": {
      "contentSize": "80rem",
      "wideSize": "96rem"
    },
    "spacing": {
      "margin": false,
      "padding": false,
      "units": []
    },
    "typography": {
      "customFontSize": false,
      "dropCap": false,
      "fontStyle": true,
      "fontWeight": true,
      "letterSpacing": false,
      "lineHeight": false,
      "textColumns": false,
      "textDecoration": true,
      "textTransform": true,
      "writingMode": false,
      "fontFamilies": [
        {
          "name": "Geist",
          "slug": "geist",
          "fontFamily": "Geist, sans-serif",
          "fontFace": [
            {
              "fontFamily": "Geist",
              "fontWeight": "100 900",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "fontDisplay": "swap",
              "src": [ "file:./assets/fonts/Geist/Geist[wght].woff2" ]
            }
          ]
        }
      ],
      "fluid": false,
      "fontSizes": []
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--geist)"
    },
    "elements": {
      "button": {
        "border": {
          "color": "var(--wp--preset--color--primary)",
          "width": "1px",
          "style": "solid",
          "radius": "12px"
        },
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--foreground)"
        },
        ":hover": {
          "color": {
            "background": "transparent",
            "text": "var(--wp--preset--color--foreground)"
          }
        },
        ":focus": {
          "color": {
            "background": "transparent",
            "text": "var(--wp--preset--color--foreground)"
          }
        },
        ":active": {
          "color": {
            "background": "transparent",
            "text": "var(--wp--preset--color--foreground)"
          }
        },
        "typography": {
          "fontWeight": "700",
          "lineHeight": "1.5"
        }
      }
    }
  }
}
